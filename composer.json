{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        },
        {
            "name": "Pierre Berube",
            "email": "pierre@lgse.com",
            "homepage": "http://www.lgse.com/"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "doctrine/dbal": "^4",
        "doctrine/migrations": "^3.9",
        "doctrine/orm": "^3",
        "monolog/monolog": "^3.9",
        "php-di/php-di": "^6.4",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10",
        "symfony/cache": "^7",
        "symfony/property-access": "^7.3",
        "symfony/serializer": "^7.3"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.2.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.26",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/maker-bundle": "^1.64"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "process-timeout": 0,
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit",
        "pwd": "pwd",
        "migrations": "./vendor/bin/doctrine-migrations",
        "migrations:diff": "vendor/bin/doctrine-migrations migrations:diff",
        "migrations:generate": "vendor/bin/doctrine-migrations migrations:generate",
        "migrations:migrate": "vendor/bin/doctrine-migrations migrations:migrate"
        }
}
